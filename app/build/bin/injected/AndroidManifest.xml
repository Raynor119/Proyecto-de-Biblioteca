<?xml version="1.0" encoding="utf-8"?>
<manifest
	xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.pixels.bdpbiblio"
	android:versionCode="1"
	android:versionName="1.0">

	<uses-sdk
		android:minSdkVersion="14"
		android:targetSdkVersion="27"/>

	<uses-permission
		android:name="android.permission.INTERNET"/>

	<application
		android:allowBackup="true"
		android:icon="@mipmap/ic_launcher"
		android:label="@string/app_name"
		android:theme="@style/AppTheme"
		android:resizeableActivity="true">

		<activity
			android:name=".MainActivity"
			android:theme="@style/AppTheme.NoActionBar">

		</activity>

		<activity
			android:name=".inicio"
			android:theme="@style/AppTheme.NoActionBar">

			<intent-filter>

				<action
					android:name="android.intent.action.MAIN"/>

				<category
					android:name="android.intent.category.LAUNCHER"/>

			</intent-filter>

		</activity>

		<activity
			android:name=".multa"
			android:theme="@style/AppTheme.NoActionBar"/>

		<activity
			android:name=".ItemDetailActivity2"
			android:theme="@style/AppTheme.NoActionBar">

			<meta-data
				android:name="android.support.PARENT_ACTIVITY"
				android:value="com.pixels.bdpbiblio.multa"/>

		</activity>

		<activity
			android:name=".devolucion"
			android:theme="@style/AppTheme.NoActionBar"/>

		<activity
			android:name=".ItemDetailActivity1"
			android:theme="@style/AppTheme.NoActionBar">

			<meta-data
				android:name="android.support.PARENT_ACTIVITY"
				android:value="com.pixels.bdpbiblio.devolucion"/>

		</activity>

		<activity
			android:name=".prestamos"
			android:theme="@style/AppTheme.NoActionBar"/>

		<activity
			android:name=".ItemDetailActivity"
			android:theme="@style/AppTheme.NoActionBar">

			<meta-data
				android:name="android.support.PARENT_ACTIVITY"
				android:value="com.pixels.bdpbiblio.prestamos"/>

		</activity>

		<activity
			android:name=".agregarprestamou"
			android:theme="@style/calendario"/>

	</application>

</manifest>

